{
  "manifest_version": 2,

  "name": "Kupla",
  "description": "Password Extension",
  "version": "1.0",
  "background": {
   "scripts": ["popup.js"],
   "persistent": false
  },
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
   
  },
  "permissions": [
   "activeTab"
   ]

}